  <body>
    {% if shouldIncludeTracking %}
      <!-- Google Tag Manager (noscript) -->
      <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PRPJKWN"
      height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
      <!-- End Google Tag Manager (noscript) -->
    {% endif %}

    {% block before_header %}
    {% endblock %}
    <div class="usa-banner">
      <header class="usa-banner-header">
        <div class="usa-grid usa-banner-inner">
          <img src="/images/favicon-57.png" alt="U.S. flag">
          <p>An official website of the United States government</p>
        </div>
      </header>
    </div>
    <header class="usa-header usa-header-basic" role="banner">
      <div class="usa-nav-container">
        <div class="usa-navbar">
          <div class="usa-logo" id="basic-logo">
            <em class="usa-logo-text">
              <a href="/" title="Home" aria-label="Home">Federalist</a>
            </em>
          </div>
          <button class="usa-menu-btn">Menu</button>
        </div>

        <nav role="navigation" class="usa-nav">
          <button class="usa-nav-close">
            <img src="/images/close.svg" alt="close">
          </button>
          <ul class="usa-nav-primary usa-accordion">
            {% if not isAuthenticated %}
              <li>
                <a class="usa-nav-link" href="/content/examples/" rel="noopener noreferrer"><span>Example sites</span></a>
              </li>
              <li>
                <a class="usa-nav-link" href="https://federalist-docs.18f.gov" target="_blank" rel="noopener noreferrer"><span>Documentation</span></a>
              </li>
              <li>
                <a class="usa-nav-link" href="https://docs.google.com/forms/d/e/1FAIpQLSesNI1qlov7Ec_1u2FralYWg9hV4WsB-3FyAHPXc1pRT6In7w/viewform" target="_blank" rel="noopener noreferrer"><span>Get started</span></a>
              </li>
              <li>
                <a href="/auth/github" class="usa-button"><span>Log in</span></a>
              </li>
            {% else %}
              <li>
                <a class="usa-nav-link" href="/sites"><span>{{ username }}'s sites</span></a>
              </li>
              <li>
                <a class="usa-nav-link" href="https://federalist-docs.18f.gov" target="_blank" rel="noopener noreferrer"><span>Documentation</span></a>
              </li>
              <li>
                <a class="usa-nav-link" href="https://github.com/18F/federalist/issues/new" target="_blank" rel="noopener noreferrer"><span>File issue</span></a>
              </li>
              <li>
                <a class="usa-nav-link" href="mailto:federalist-support@gsa.gov"><span>Get help</span></a>
              </li>
              <li>
                <a class="usa-button" href="/logout"><span>Log out</span></a>
              </li>
            {% endif %}
          </ul>
        </nav>
      </div>
    </header>
    <div class="usa-overlay"></div>
    {% block flash_messages %}
      {% if messages.errors %}
        {% for error in messages.errors %}
        <div class="usa-alert-home">
          <div class="usa-alert usa-alert-error usa-alert-slim" role="alert">
            <div class="usa-alert-body">
              <p class="usa-alert-text"><b>{{ error.title }}</b> {{ error.message }}</p>
            </div>
          </div>
        </div>
        {% endfor %}
      {% endif %}
    {% endblock %}

    {% block main_content %}
    {% endblock %}

